    "import turtle\n",
    "\n",
    "h = turtle.Turtle()\n",
    "\n",
    "# set pen color\n",
    "turtle.pencolor(\"#00BFFF\")\n",
    "# draw the house\n",
    "h.goto(-300, -300)\n",
    "h.lt(90)\n",
    "for i in range(2):\n",
    "    h.fd(250)\n",
    "    h.rt(90)\n",
    "    h.fd(200)\n",
    "    h.rt(90)\n",
    "    \n",
    "# set background\n",
    "turtle.bgcolor(\"#00BFFF\")\n",
    "turtle.done()"
