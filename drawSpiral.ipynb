   "source": [
    "import turtle\n",
    "\n",
    "t = turtle.Turtle()\n",
    "\n",
    "time_execute = int(input(\"Enter value of time \"))\n",
    "i = 0\n",
    "\n",
    "while i < time_execute:\n",
    "    i += 1\n",
    "    t.fd(i)\n",
    "    t.rt(30)\n",
    "else:\n",
    "    turtle.done()"
   ]
